{
  "name": "@shareandcharge/ocn-registry",
  "version": "1.0.0",
  "description": "Share & Charge registry smart contracts",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/library.js",
  "bin": {
    "ocn-registry": "dist/cli.js"
  },
  "files": [
    "*.json",
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "compile": "truffle compile",
    "test": "rm -Rf build && truffle test",
    "migrate": "truffle migrate",
    "migrate-docker": "truffle migrate --network=docker",
    "ganache": "ganache-cli -m \"candy maple cake sugar pudding cream honey rich smooth crumble sweet treat\" --port 8544 --accounts 20 --networkId=9 --gasLimit=10000000",
    "publish-dev": "truffle migrate && node bin/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org:shareandcharge/registry.git"
  },
  "author": "Share & Charge foundation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://bitbucket.org/shareandcharge/registry/issues?status=new&status=open"
  },
  "homepage": "https://bitbucket.org/shareandcharge/registry",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/yargs": "^15.0.4",
    "ganache-cli": "^6.9.1",
    "mkdirp": "^0.5.1",
    "openzeppelin-solidity": "^2.5.1",
    "truffle": "^5.1.25",
    "truffle-contract": "^4.0.31",
    "truffle-flattener": "^1.4.4",
    "truffle-hdwallet-provider": "^1.0.17",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bignumber.js": "^8.0.2",
    "ethers": "^4.0.47",
    "web3-utils": "^1.2.7",
    "yargs": "^15.3.1"
  }
}
